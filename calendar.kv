<Button>:
	background_color: 0.1,.5,.5,1
	
<Calender_Part>:
	canvas.before:
		Color:
			rgba: .5,.6,.7,1
		Rectangle:
			pos: self.pos
			size: self.size
	orientation:'vertical'
	BoxLayout:
		size_hint:1,.1
		Select:
			id: select
		
	BoxLayout:
		Months:
			id: months
		BoxLayout:
			orientation: 'vertical'
			Days:
				id: days
			Dates:
				id: dates
	